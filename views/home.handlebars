<form action="/tarefas" method="post">

<p>Qual sua tarefa?</p>
<input type="text" name="descricao">

<button type="submit">Adicionar</button>
</form>

<h2>Lista de Tarefas:</h2>

    {{#each tarefas}}
            <p>{{descricao}}</p>
            <button>deletar</button>
            <hr>
    {{/each}}